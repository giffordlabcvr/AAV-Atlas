<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>aavMaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<useSingleReference>true</useSingleReference>

	<cladeCategory>
		<name>species</name>	
		<displayName>Species</displayName>	
		<whereClause>clade_category = 'species'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.4</distanceCutoff>
		<finalCladeCutoff>75.0</finalCladeCutoff>		
	</cladeCategory>

	<cladeCategory>
		<name>serotype</name>	
		<displayName>Serotype</displayName>	
		<whereClause>clade_category = 'serotype'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.05</distanceCutoff>
		<finalCladeCutoff>75.0</finalCladeCutoff>		
	</cladeCategory>

</maxLikelihoodGenotyper>

