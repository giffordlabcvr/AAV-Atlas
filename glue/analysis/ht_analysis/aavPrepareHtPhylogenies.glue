  delete module aavPhyloUtility
  delete module aavFigTreeAnnotationExporter
  
  create module -f modules/aavPhyloUtility.xml
  
  module aavPhyloUtility
  
	# midpoint rooting

	reroot-phylogeny \
      --inputFile trees/ht-capsid/rep78_00045_nucs_mammalsOnly.tree NEWICK_BOOTSTRAPS \
      --midpoint \
      --outputFile trees/ht-capsid/rep78_00045_nucs_mammalsOnly_MidpointRerooted.tree NEWICK_BOOTSTRAPS

	reroot-phylogeny \
      --inputFile trees/ht-capsid/rep78_00045_nucs_avianOutgrp.tree NEWICK_BOOTSTRAPS \
      --midpoint \
      --outputFile trees/ht-capsid/rep78_00045_nucs_avianOutgrp_MidpointRerooted.tree NEWICK_BOOTSTRAPS

    exit
    
  
  # Export annotations
  create module -f modules/aavFigTreeAnnotationExporter.xml
  module aavFigTreeAnnotationExporter export figtree-annotation AL_AAV_HT_ANALYSIS_MammalsOnly -f trees/ht-capsid/aavHtPhylogeniesMammalsOnly.annotations.tsv
  module aavFigTreeAnnotationExporter export figtree-annotation AL_AAV_HT_ANALYSIS_AvAavOutgrp -f trees/ht-capsid/aavHtPhylogeniesAvAavOutgrp.annotations.tsv
