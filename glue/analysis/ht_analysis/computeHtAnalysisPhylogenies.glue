#~# Build nucleotide trees (Rep78)

module aavRaxmlPhylogenyGeneratorNucs

  # Mammals-only dataset
  generate nucleotide phylogeny AL_AAV_HT_ANALYSIS_MammalsOnly \
    -s aavAlignmentColumnsHtAnalysis-Rep78_00045_Nuc \
    -a -o analyses/ht_analysis/trees/rep78_00045_nucs_mammalsOnly.tree NEWICK_BOOTSTRAPS

  # Avian outgroup dataset
  generate nucleotide phylogeny AL_AAV_HT_ANALYSIS_AvAavOutgrp \
    -s aavAlignmentColumnsHtAnalysis-Rep78_00045_Nuc \
    -a -o analyses/ht_analysis/trees/rep78_00045_nucs_avianOutgrp.tree NEWICK_BOOTSTRAPS

exit


