#~# Build nucleotide trees (Rep78)

module aavRaxmlPhylogenyGeneratorNucs

  # Mammals-only dataset
  generate nucleotide phylogeny AL_AAV_HT_ANALYSIS_AllTaxa_NoOutgrp \
    -s aavAlignmentColumnsHtAnalysis-Rep78_00045_Nuc \
    -a -o analyses/ht_analysis/trees/rep78_00045_nucs_allTaxa_noOutgrp.tree NEWICK_BOOTSTRAPS

  # Avian outgroup dataset
  generate nucleotide phylogeny AL_AAV_HT_ANALYSIS_AllTaxa_AvAavOutgrp \
    -s aavAlignmentColumnsHtAnalysis-Rep78_00045_Nuc \
    -a -o analyses/ht_analysis/trees/rep78_00045_nucs_allTaxa_avianOutgrp.tree NEWICK_BOOTSTRAPS

exit


